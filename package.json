{
  "name": "@ekstra-bladet/designsystem",
  "version": "0.8.1",
  "main": "src/index.ts",
  "repository": "git@github.com:EkstraBladetUdvikling/eb-designsystem.git",
  "author": "tegner <jesper.h.tegner@jppol.dk>",
  "files": [
    "./dist",
    "./snippets",
    "./src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:designsystem && yarn svgs && yarn build:svelte",
    "build:designsystem": "yarn build:css && yarn build:watcher && yarn build:snippets",
    "build:css": "node node/build-css.js",
    "build:nonie": "node node/build-css.js --nonie",
    "build:ie": "node node/build-css.js --ie",
    "build:snippets": "node node/build-snippets",
    "build:watch": "onchange 'src/**/*.css' -- yarn build:watcher",
    "build:watcher": "yarn build:css --watcher",
    "build:svelte": "yarn rollup -c --silent",
    "dev": "yarn build:watcher & yarn build:watch & yarn rollup -c -w",
    "predev": "yarn svgs",
    "start": "sirv docs",
    "validate": "svelte-check",
    "dist": "yarn build",
    "prepublish": "yarn dist",
    "dist:move": "yarn dist && yarn move_to_eb",
    "installds": "node node/install-system",
    "installds:eb": "node node/nstall-system ../ekstrabladet/ekstrabladet-publication/src/main/frontend/css-main/main/css",
    "installsnippets": "node node/install-snippets",
    "installsnippets:eb": "node node/install-snippets ../ekstrabladet/ekstrabladet-publication/src/main/.vscode",
    "move_to_eb": "yarn installds:eb",
    "copy_svg": "copyfiles -f \"./docs/svg/symbol/icons.svg\" \"./dist\"",
    "start:designsystem": "yarn build:watcher && yarn build:watch",
    "svgs": "node node/svgs.js && yarn copy_svg"
  },
  "dependencies": {
    "@ekstra-bladet/eb-colors": "^3.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "prismjs": "^1.23.0",
    "rollup": "^2.50.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.38.2",
    "svelte-check": "^1.6.0",
    "svelte-preprocess": "^4.7.3",
    "svelte-prism": "^1.1.3",
    "svelte-spa-router": "^3.1.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "csscomb": "^4.3.0",
    "normalize.css": "^8.0.1",
    "onchange": "^7.1.0",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-discard-duplicates": "^5.0.1",
    "postcss-discard-unused": "^5.0.1",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.0.1",
    "postcss-merge-rules": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-selectorcleanse": "^1.0.6",
    "svg-sprite": "^1.5.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
