{
  "name": "@ekstra-bladet/designsystem",
  "version": "7.3.0",
  "main": "svelte/index.js",
  "repository": "git@github.com:EkstraBladetUdvikling/eb-designsystem.git",
  "author": "tegner <jesper.h.tegner@jppol.dk>",
  "files": [
    "./css",
    "./snippets",
    "./svelte"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn lint && yarn validate && rm -rf ./dist && yarn build:typescript && yarn build:designsystem && yarn svgs && yarn build:docs",
    "build:css": "node node/build-css.js",
    "build:designsystem": "yarn build:css && yarn build:snippets",
    "build:docs": "yarn rollup -c --bundleConfigAsCjs",
    "build:snippets": "rm -rf ./snippets && node node/build-snippets",
    "build:typescript": "rm -rf ./svelte && node node/move-svelte && yarn tsc --project tsconfig.tsc.json",
    "build:watch": "onchange 'src/**/*.css' -- yarn build:css",
    "dev": "yarn build:typescript && yarn build:css & yarn build:watch & yarn rollup -c -w --bundleConfigAsCjs",
    "predev": "yarn svgs",
    "start": "sirv docs",
    "validate": "svelte-check || { echo 'svelte-check failed' ; exit: 1; }",
    "dist": "yarn build",
    "lint": "node node/linter.mjs || { echo 'linting failed' ; exit: 1; }",
    "prepare": "yarn build:typescript",
    "prepublish": "yarn dist",
    "installsnippets": "node node/install-snippets",
    "installsnippets:eb": "node node/install-snippets ../ekstrabladet/ekstrabladet-publication/src/main/.vscode",
    "copy_svg": "copyfiles -f \"./docs/svg/symbol/icons.svg\" \"./dist\"",
    "svgs": "node node/svgs.js && yarn copy_svg"
  },
  "dependencies": {
    "@ekstra-bladet/eb-colors": "4.0.5",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@tsconfig/svelte": "^4.0.1",
    "fitty": "^2.3.5",
    "prismjs": "^1.28.0",
    "rollup": "^3.20.6",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.4",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.6",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2",
    "svelte": "3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-prism": "^1.1.5",
    "svelte-spa-router": "^3.2.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^43.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "normalize.css": "^8.0.1",
    "onchange": "^7.1.0",
    "postcss": "^8.4.23",
    "postcss-custom-media": "^9.1.3",
    "postcss-custom-properties": "^13.1.5",
    "postcss-discard-duplicates": "^6.0.0",
    "postcss-discard-unused": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-merge-rules": "^6.0.0",
    "postcss-preset-env": "^8.3.2",
    "postcss-selectorcleanse": "^1.0.6",
    "svg-sprite": "^2.0.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
}
