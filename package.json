{
  "name": "@ekstra-bladet/designsystem",
  "version": "0.7.1",
  "main": "dist/eb-designsystem.css",
  "repository": "git@github.com:EkstraBladetUdvikling/eb-designsystem.git",
  "author": "tegner <jesper.h.tegner@jppol.dk>",
  "files": [
    "./dist",
    "./snippets",
    "./src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:designsystem && yarn svgs && yarn build:svelte",
    "build:designsystem": "yarn build:css && yarn build:watcher && yarn build:snippets",
    "build:css": "node node/build-css.js",
    "build:nonie": "node node/build-css.js --nonie",
    "build:ie": "node node/build-css.js --ie",
    "build:preview": "postcss preview/css/_index.css -o docs/css/main.css --no-map -u postcss-import",
    "build:snippets": "node node/build-snippets",
    "build:watch": "onchange 'src/**/*.css' -- yarn build:watcher & onchange 'preview/**/{_*.css,html}' -- yarn build:preview",
    "build:watcher": "yarn build:css --watcher && yarn build:preview",
    "build:svelte": "yarn rollup -c --silent",
    "dev": "yarn rollup -c -w & yarn build:watcher & yarn build:watch",
    "start": "sirv docs",
    "validate": "svelte-check",
    "dist": "yarn build",
    "prepublish": "yarn dist",
    "dist:move": "yarn dist && yarn move_to_eb",
    "installds": "node node/install-system",
    "installds:eb": "node node/nstall-system ../ekstrabladet/ekstrabladet-publication/src/main/frontend/css-main/main/css",
    "installsnippets": "node node/install-snippets",
    "installsnippets:eb": "node node/install-snippets ../ekstrabladet/ekstrabladet-publication/src/main/.vscode",
    "move_to_eb": "yarn installds:eb",
    "copy_svg": "cpx \"./docs/svg/symbol/icons.svg\" \"./dist\" --update",
    "start:designsystem": "yarn build:watcher && yarn build:preview && yarn build:watch",
    "svgs": "node node/svgs.js && yarn copy_svg"
  },
  "dependencies": {
    "@ekstra-bladet/eb-colors": "^3.0.0",
    "sirv-cli": "^1.0.0",
    "svelte-spa-router": "^3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.0",
    "cpx": "^1.5.0",
    "csscomb": "^4.3.0",
    "normalize.css": "^8.0.1",
    "onchange": "^7.1.0",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-discard-unused": "^4.0.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-merge-rules": "^4.0.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-selectorcleanse": "^1.0.6",
    "prismjs": "^1.23.0",
    "rollup": "^2.42.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.5",
    "svelte-preprocess": "^4.6.9",
    "svelte-prism": "^1.1.3",
    "svg-sprite": "^1.5.0",
    "tslib": "^2.0.0",
    "typescript": "^4.2.3"
  }
}
