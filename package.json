{
  "name": "@ekstra-bladet/designsystem",
  "version": "1.3.0",
  "main": "dist/index.js",
  "repository": "git@github.com:EkstraBladetUdvikling/eb-designsystem.git",
  "author": "tegner <jesper.h.tegner@jppol.dk>",
  "files": [
    "./dist",
    "./snippets"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && yarn build:typescript && yarn build:designsystem && yarn svgs && yarn build:svelte",
    "build:designsystem": "yarn build:css && yarn build:snippets",
    "build:css": "node node/build-css.js",
    "build:snippets": "node node/build-snippets",
    "build:typescript": "node node/move-svelte && yarn tsc --project tsconfig.tsc.json",
    "build:watch": "onchange 'src/**/*.css' -- yarn build:css",
    "build:svelte": "yarn rollup -c",
    "dev": "yarn build:typescript && yarn build:css & yarn build:watch & yarn rollup -c -w",
    "predev": "yarn svgs",
    "start": "sirv docs",
    "validate": "svelte-check",
    "dist": "yarn build",
    "prepublish": "yarn dist",
    "installsnippets": "node node/install-snippets",
    "installsnippets:eb": "node node/install-snippets ../ekstrabladet/ekstrabladet-publication/src/main/.vscode",
    "copy_svg": "copyfiles -f \"./docs/svg/symbol/icons.svg\" \"./dist\"",
    "svgs": "node node/svgs.js && yarn copy_svg"
  },
  "dependencies": {
    "@ekstra-bladet/eb-colors": "^4.0.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "prismjs": "^1.25.0",
    "rollup": "^2.57.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.2",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.43.0",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.9.5",
    "svelte-prism": "^1.1.3",
    "svelte-spa-router": "^3.1.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "csscomb": "^4.3.0",
    "normalize.css": "^8.0.1",
    "onchange": "^7.1.0",
    "postcss": "^8.3.7",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^12.0.0",
    "postcss-discard-duplicates": "^5.0.1",
    "postcss-discard-unused": "^5.0.1",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.0.1",
    "postcss-merge-rules": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-selectorcleanse": "^1.0.6",
    "svg-sprite": "^1.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.2.4"
  }
}
